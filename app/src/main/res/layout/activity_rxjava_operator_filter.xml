<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="test.cn.example.com.androidskill.LannchModeActivity">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava过滤操作符，结果看打印的日志"/>
        <Button
            android:id="@+id/debounce"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符Debounce的使用"/>

        <Button
            android:id="@+id/distinct"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符distinct的使用"/>

        <Button
            android:id="@+id/elementAt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符elementAt的使用"/>

        <Button
            android:id="@+id/filter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符filter的使用"/>

        <Button
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="rxjava的过滤类操作符first的使用"/>

        <Button
            android:id="@+id/ignoreElements"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符ignoreElements的使用"/>

        <Button
            android:id="@+id/last"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符last的使用"/>

        <Button
            android:id="@+id/ofType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符ofType的使用"/>

        <Button
            android:id="@+id/sample"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符sample的使用"/>

        <Button
            android:id="@+id/throttleFirst"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符throttleFirst的使用"/>

        <Button
            android:id="@+id/single"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符single的使用"/>

        <Button
            android:id="@+id/skip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符skip的使用"/>

        <Button
            android:id="@+id/skipLast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符skipLast的使用"/>

        <Button
            android:id="@+id/take"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符take的使用"/>

        <Button
            android:id="@+id/takeLast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符takeLast的使用"/>

        <Button
            android:id="@+id/takeLastBuffer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="rxjava的过滤类操作符takeLastBuffer的使用"/>
    </LinearLayout>
</ScrollView>


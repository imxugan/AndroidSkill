<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleView">
        <attr name="circle_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="LoadingIndicatorView">
        <attr name="minWidth" format="dimension"/>
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension"/>
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorColor" format="color"/>
        <attr name="indicatorName" format="string"/>
    </declare-styleable>

    <attr name="titleText" format="string"/>
    <attr name="titleTextColor" format="color"/>
    <attr name="titleTextSize" format="dimension"/>
    <declare-styleable name="CustomTitleView">
        <attr name="titleText"/>
        <attr name="titleTextColor"/>
        <attr name="titleTextSize"/>
    </declare-styleable>

    <declare-styleable name="CustomView">
        <attr name="attr1" format="string"/>
        <attr name="attr2" format="string"/>
        <attr name="attr3" format="string"/>
        <attr name="attr4" format="string"/>
        <attr name="attr5" format="string"/>
        <attr name="attr6" format="string"/>
    </declare-styleable>
    <attr name="customViewStyle" format="reference"/>

    <attr name="image" format="reference"/>
    <attr name="imageScaleType" format="enum">
        <enum name="fillXY" value="0"/>
        <enum name="center" value="1"/>
    </attr>
    <declare-styleable name="CustomImageView">
        <attr name="titleText" />
        <attr name="titleTextSize"/>
        <attr name="titleTextColor"/>
        <attr name="image"/>
        <attr name="imageScaleType"/>
    </declare-styleable>

    <attr name="firstColor" format="color"/>
    <attr name="secondColor" format="color"/>
    <attr name="circleWidth" format="dimension"/>
    <attr name="speed" format="integer"/>
    <declare-styleable name="CustomProgressBar">
        <attr name="firstColor"/>
        <attr name="secondColor"/>
        <attr name="circleWidth"/>
        <attr name="speed"/>
    </declare-styleable>

    <attr name="count" format="integer"/>
    <attr name="splitSize" format="integer"/>
    <declare-styleable name="CustomVolumeControlBar">
        <attr name="firstColor"/>
        <attr name="secondColor"/>
        <attr name="circleWidth"/>
        <attr name="count"/>
        <attr name="image"/>
        <attr name="splitSize"/>
    </declare-styleable>

    <attr name="custom_text" format="string"/>
    <attr name="custom_testAttr" format="integer"/>
    <declare-styleable name="test">
        <attr name="text" format="string"/>
        <attr name="testAttr" format="integer"/>
        <!--使用系统自带的属性，注意这里没有format了-->
        <!--布局文件中直接android:text="@string/hello_world"即可-->
        <attr name="android:text"/>
    </declare-styleable>

    <declare-styleable name="GestureLockView">
        <attr name="no_finger_inner_circle_color" format="color"/>
        <attr name="no_finger_outer_circle_color" format="color"/>
        <attr name="finger_on_color" format="color"/>
        <attr name="finger_up_color" format="color"/>
        <attr name="count"/>
        <attr name="tryTimes" format="integer"/>
    </declare-styleable>

    <declare-styleable name="GestureLockViewGroup">
        <attr name="no_finger_inner_circle_color"/>
        <attr name="no_finger_outer_circle_color"/>
        <attr name="finger_on_color"/>
        <attr name="finger_up_color"/>
        <attr name="count"/>
        <attr name="tryTimes"/>
    </declare-styleable>

    <declare-styleable name="GestureLockView2">
        <attr name="inner_circle_color" format="color"/>
        <attr name="out_circle_color" format="color"/>
        <attr name="arrow_circle_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ArcMenuViewGroup">
        <attr name="arcMenuRadius" format="dimension"/>
        <attr name="arcMenuCount" format="integer"/>
        <attr name="degree" format="integer"/>
        <attr name="position" format="enum">
            <enum name="left_top" value="0"/>
            <enum name="right_top" value="1"/>
            <enum name="left_botttom" value="2"/>
            <enum name="right_bottom" value="3"/>
        </attr>
    </declare-styleable>

    <attr name="a_in" format="float"/>
    <attr name="a_out" format="float"/>
    <attr name="x_in" format="float"/>
    <attr name="x_out" format="float"/>
    <attr name="y_in" format="float"/>
    <attr name="y_out" format="float"/>
</resources>